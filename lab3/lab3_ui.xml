<?xml version="1.0" encoding="utf-8"?>
<egt>
	<theme>
		<property name="type">MidnightTheme</property>
	</theme>
	<widgets>
		<widget name="Window0" type="TopWindow">
			<property name="pixelformat">argb8888</property>
